<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Support.MyTicketsRecyclerViewFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/subLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/colorWhite"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.drawerlayout.widget.DrawerLayout
            android:id="@+id/drawerLayout"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@color/colorWhite"
            app:layout_constraintBottom_toTopOf="@+id/bottomNavigationView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/colorWhite"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/colorWhite"
                    android:orientation="vertical">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/colorWhite">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/myTicketsLayout"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@color/colorWhite"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent">

                            <TextView
                                android:id="@+id/MyTicketsTitle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="24dp"
                                android:layout_marginLeft="24dp"
                                android:layout_marginTop="32dp"
                                android:letterSpacing="0.1"
                                android:text="@string/my_tickets"
                                android:textColor="@color/colorZambezi"
                                android:textSize="14dp"
                                app:fontFamily="@font/roboto_bold"
                                app:layout_constraintStart_toEndOf="@+id/leftArrowMark"
                                app:layout_constraintTop_toTopOf="parent" />

                            <ImageView
                                android:id="@+id/leftArrowMark"
                                android:layout_width="12dp"
                                android:layout_height="12dp"
                                android:layout_marginStart="36dp"
                                android:layout_marginLeft="36dp"
                                android:layout_marginTop="36dp"
                                android:src="@drawable/back_arrow_icon_xxhdpi"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                            <ImageView
                                android:id="@+id/addIcon"
                                android:layout_width="34dp"
                                android:layout_height="34dp"
                                android:layout_marginTop="24dp"
                                android:layout_marginEnd="32dp"
                                android:layout_marginRight="32dp"
                                android:src="@drawable/add_icon_xxhdpi"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/searchMainLayout"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/myTicketsLayout">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:id="@+id/searchLayout"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="32dp"
                                android:layout_marginLeft="32dp"
                                android:layout_marginTop="42dp"
                                android:layout_marginEnd="32dp"
                                android:layout_marginRight="32dp"
                                android:layout_marginBottom="32dp"
                                android:background="@drawable/search"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent">

                                <EditText
                                    android:id="@+id/search"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginLeft="20dp"
                                    android:background="@color/colorWhite"
                                    android:ems="24"
                                    android:hint="@string/search"
                                    android:inputType="textPassword"
                                    android:letterSpacing="0.07"
                                    android:paddingTop="2dp"
                                    android:paddingBottom="2dp"
                                    android:textColorHint="@color/colorSuvaGray"
                                    android:textSize="10dp"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent"></EditText>

                                <ImageView
                                    android:id="@+id/searchIcon"
                                    android:layout_width="17dp"
                                    android:layout_height="17dp"
                                    android:layout_marginTop="9dp"
                                    android:layout_marginEnd="12dp"
                                    android:layout_marginRight="12dp"
                                    android:layout_marginBottom="9dp"
                                    android:src="@drawable/search_icon_xxhdpi"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintTop_toTopOf="parent">

                                </ImageView>

                            </androidx.constraintlayout.widget.ConstraintLayout>


                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/filterIconLayout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/searchMainLayout">

                            <ImageView
                                android:id="@+id/filterIcon"
                                android:layout_width="34dp"
                                android:layout_height="34dp"
                                android:layout_marginStart="32dp"
                                android:layout_marginLeft="32dp"
                                android:layout_marginTop="4dp"
                                android:layout_marginBottom="4dp"
                                android:src="@drawable/filter_icon_xxhdpi"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="28dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/filterIconLayout">

                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/myTicketsRecyclerView"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent">

                            </androidx.recyclerview.widget.RecyclerView>
                        </androidx.constraintlayout.widget.ConstraintLayout>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </LinearLayout>
            </ScrollView>


            <com.google.android.material.navigation.NavigationView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="start"
                android:background="@color/colorWhite"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingBottom="100dp">

                            <ImageView
                                android:id="@+id/FilterIamge"
                                android:layout_width="19dp"
                                android:layout_height="19dp"
                                android:layout_marginTop="50dp"
                                android:src="@drawable/filter_image_xxhdpi"
                                app:layout_constraintStart_toStartOf="@+id/guideline5"
                                app:layout_constraintTop_toTopOf="parent">

                            </ImageView>

                            <TextView
                                android:id="@+id/filter"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="16dp"
                                android:layout_marginLeft="16dp"
                                android:layout_marginTop="48dp"
                                android:letterSpacing="0.05"
                                android:text="@string/filter"
                                android:textColor="@color/colorZambezi1"
                                android:textSize="14dp"
                                app:fontFamily="@font/roboto_medium"
                                app:layout_constraintStart_toEndOf="@+id/FilterIamge"
                                app:layout_constraintTop_toTopOf="parent" />

                            <androidx.constraintlayout.widget.Guideline
                                android:id="@+id/guideline5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="24dp"
                                android:layout_marginLeft="24dp"
                                android:orientation="vertical"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintGuide_begin="24dp"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:id="@+id/clearAllLayout"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="24dp"
                                android:layout_marginEnd="90dp"
                                android:layout_marginRight="90dp"
                                android:background="@color/colorWhisper4"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="@+id/guideline5"
                                app:layout_constraintTop_toBottomOf="@+id/FilterIamge">

                                <TextView
                                    android:id="@+id/clearAll"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="24dp"
                                    android:layout_marginLeft="24dp"
                                    android:layout_marginTop="8dp"
                                    android:letterSpacing="0.05"
                                    android:text="@string/clear_all"
                                    android:textColor="@color/colorPinkSwan"
                                    android:textSize="13dp"
                                    app:fontFamily="@font/roboto_bold"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <TextView
                                    android:id="@+id/clearDescription"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="24dp"
                                    android:layout_marginLeft="24dp"
                                    android:layout_marginTop="2dp"
                                    android:layout_marginBottom="8dp"
                                    android:letterSpacing="0.04"
                                    android:text="@string/filter_description"
                                    android:textColor="@color/colorSuvaGray"
                                    android:textSize="13dp"
                                    app:fontFamily="@font/roboto_medium"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toBottomOf="@+id/clearAll" />
                            </androidx.constraintlayout.widget.ConstraintLayout>

                            <TextView
                                android:id="@+id/status"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="42dp"
                                android:letterSpacing="0.06"
                                android:text="@string/status"
                                android:textColor="@color/colorCharcoal"
                                android:textSize="12dp"
                                app:fontFamily="@font/roboto_medium"
                                app:layout_constraintStart_toStartOf="@+id/guideline5"
                                app:layout_constraintTop_toBottomOf="@+id/clearAllLayout" />

                            <View
                                android:id="@+id/statusDivider"
                                android:layout_width="0dp"
                                android:layout_height="0.6dp"
                                android:layout_marginTop="8dp"
                                android:layout_marginEnd="60dp"
                                android:layout_marginRight="60dp"
                                android:background="@color/colorVeryLightGray"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="@+id/guideline5"
                                app:layout_constraintTop_toBottomOf="@+id/status"></View>


                            <LinearLayout
                                android:id="@+id/openCheckBox"
                                android:layout_width="12dp"
                                android:layout_height="12dp"
                                android:layout_marginTop="24dp"
                                android:background="@drawable/custom_checkbox"
                                android:orientation="vertical"
                                app:layout_constraintStart_toStartOf="@+id/guideline5"
                                app:layout_constraintTop_toBottomOf="@+id/statusDivider"></LinearLayout>

                            <TextView
                                android:id="@+id/open"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="16dp"
                                android:layout_marginLeft="16dp"
                                android:layout_marginTop="20dp"
                                android:letterSpacing="0.07"
                                android:text="@string/open"
                                android:textColor="@color/colorDimGray"
                                android:textSize="12dp"
                                app:fontFamily="@font/roboto_medium"
                                app:layout_constraintStart_toEndOf="@+id/openCheckBox"
                                app:layout_constraintTop_toBottomOf="@+id/statusDivider" />

                            <TextView
                                android:id="@+id/openDescription"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="30dp"
                                android:layout_marginLeft="30dp"
                                android:layout_marginTop="6dp"
                                android:letterSpacing="0.08"
                                android:text="Tickets that are assigned but not closed."
                                android:textColor="@color/colorDimGray"
                                android:textSize="10dp"
                                app:fontFamily="@font/roboto_italic"
                                app:layout_constraintStart_toStartOf="@+id/guideline5"
                                app:layout_constraintTop_toBottomOf="@+id/open" />


                            <LinearLayout
                                android:id="@+id/closedCheckBox"
                                android:layout_width="12dp"
                                android:layout_height="12dp"
                                android:layout_marginTop="44dp"
                                android:background="@drawable/custom_checkbox"
                                android:orientation="vertical"
                                app:layout_constraintStart_toStartOf="@+id/guideline5"
                                app:layout_constraintTop_toBottomOf="@+id/openCheckBox"></LinearLayout>

                            <TextView
                                android:id="@+id/closed"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="16dp"
                                android:layout_marginLeft="16dp"
                                android:layout_marginTop="20dp"
                                android:letterSpacing="0.07"
                                android:text="@string/closeed"
                                android:textColor="@color/colorDimGray"
                                android:textSize="12dp"
                                app:fontFamily="@font/roboto_medium"
                                app:layout_constraintStart_toEndOf="@+id/closedCheckBox"
                                app:layout_constraintTop_toBottomOf="@+id/openDescription" />


                            <TextView
                                android:id="@+id/closedDescription"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="30dp"
                                android:layout_marginLeft="30dp"
                                android:layout_marginTop="6dp"
                                android:letterSpacing="0.08"
                                android:text="Tickets that have been closed."
                                android:textColor="@color/colorDimGray"
                                android:textSize="10dp"
                                app:fontFamily="@font/roboto_italic"
                                app:layout_constraintStart_toStartOf="@+id/guideline5"
                                app:layout_constraintTop_toBottomOf="@+id/closed" />

                            <LinearLayout
                                android:id="@+id/draftCheckBox"
                                android:layout_width="12dp"
                                android:layout_height="12dp"
                                android:layout_marginTop="44dp"
                                android:background="@drawable/custom_checkbox"
                                android:orientation="vertical"
                                app:layout_constraintStart_toStartOf="@+id/guideline5"
                                app:layout_constraintTop_toBottomOf="@+id/closedCheckBox"></LinearLayout>

                            <TextView
                                android:id="@+id/textView20"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="16dp"
                                android:layout_marginLeft="16dp"
                                android:layout_marginTop="20dp"
                                android:letterSpacing="0.07"
                                android:text="@string/draft"
                                android:textColor="@color/colorDimGray"
                                android:textSize="12dp"
                                app:fontFamily="@font/roboto_medium"
                                app:layout_constraintStart_toEndOf="@+id/draftCheckBox"
                                app:layout_constraintTop_toBottomOf="@+id/closedDescription" />

                            <TextView
                                android:id="@+id/draftDescription"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="30dp"
                                android:layout_marginLeft="30dp"
                                android:layout_marginTop="6dp"
                                android:letterSpacing="0.08"
                                android:text="Tickets that are saved but not submitted."
                                android:textColor="@color/colorDimGray"
                                android:textSize="10dp"
                                app:fontFamily="@font/roboto_italic"
                                app:layout_constraintStart_toStartOf="@+id/guideline5"
                                app:layout_constraintTop_toBottomOf="@+id/textView20" />

                            <TextView
                                android:id="@+id/submittedDate"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="44dp"
                                android:letterSpacing="0.06"
                                android:text="@string/submitted_date"
                                android:textColor="@color/colorCharcoal"
                                android:textSize="12dp"
                                app:fontFamily="@font/roboto_medium"
                                app:layout_constraintStart_toStartOf="@+id/guideline5"
                                app:layout_constraintTop_toBottomOf="@+id/draftDescription" />

                            <View
                                android:id="@+id/submittedDateDivider"
                                android:layout_width="0dp"
                                android:layout_height="0.6dp"
                                android:layout_marginTop="8dp"
                                android:layout_marginEnd="60dp"
                                android:layout_marginRight="60dp"
                                android:background="@color/colorVeryLightGray"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="@+id/guideline5"
                                app:layout_constraintTop_toBottomOf="@+id/submittedDate"></View>

                            <View
                                android:id="@+id/submittedDateDivider2"
                                android:layout_width="0dp"
                                android:layout_height="0.6dp"
                                android:layout_marginTop="40dp"
                                android:background="@color/colorVeryLightGray"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.0"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/todayLayout"></View>

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:id="@+id/todayLayout"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="32dp"
                                android:background="@drawable/today"
                                android:elevation="1dp"
                                android:shadowColor="#00000033"
                                app:layout_constraintStart_toStartOf="@+id/guideline5"
                                app:layout_constraintTop_toBottomOf="@id/submittedDateDivider">

                                <TextView
                                    android:id="@+id/todayTitle"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="16dp"
                                    android:layout_marginLeft="16dp"
                                    android:layout_marginTop="8dp"
                                    android:layout_marginBottom="8dp"
                                    android:letterSpacing="0.04"
                                    android:text="@string/today"
                                    android:textColor="@color/colorDimGray"
                                    android:textSize="11dp"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <ImageView
                                    android:id="@+id/todayDropDownIcon"
                                    android:layout_width="9dp"
                                    android:layout_height="9dp"
                                    android:layout_marginStart="90dp"
                                    android:layout_marginLeft="90dp"
                                    android:layout_marginEnd="16dp"
                                    android:layout_marginRight="16dp"
                                    android:src="@drawable/black_drop_down"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintStart_toEndOf="@+id/todayTitle"
                                    app:layout_constraintTop_toTopOf="parent" />

                            </androidx.constraintlayout.widget.ConstraintLayout>

                            <ImageView
                                android:id="@+id/sortIcon"
                                android:layout_width="19dp"
                                android:layout_height="19dp"
                                android:layout_marginTop="36dp"
                                android:src="@drawable/filter_sort_icon_xxhdpi"
                                app:layout_constraintStart_toStartOf="@+id/guideline5"
                                app:layout_constraintTop_toBottomOf="@+id/submittedDateDivider2">

                            </ImageView>

                            <TextView
                                android:id="@+id/sort"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="16dp"
                                android:layout_marginLeft="16dp"
                                android:layout_marginTop="36dp"
                                android:letterSpacing="0.05"
                                android:text="@string/sort"
                                android:textColor="@color/colorZambezi1"
                                android:textSize="14dp"
                                app:fontFamily="@font/roboto_medium"
                                app:layout_constraintStart_toEndOf="@+id/sortIcon"
                                app:layout_constraintTop_toBottomOf="@+id/submittedDateDivider2" />

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:id="@+id/constraintLayout"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="38dp"
                                android:layout_marginEnd="90dp"
                                android:layout_marginRight="90dp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/submittedDateDivider2">

                                <TextView
                                    android:id="@+id/textView"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="8dp"
                                    android:layout_marginRight="8dp"
                                    android:letterSpacing="0.05"
                                    android:text="@string/ascending"
                                    android:textColor="@color/colorDimGray"
                                    android:textSize="10dp"
                                    app:fontFamily="@font/roboto_medium"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <ImageView
                                    android:id="@+id/imageView"
                                    android:layout_width="15dp"
                                    android:layout_height="15dp"
                                    android:layout_marginEnd="16dp"
                                    android:layout_marginRight="16dp"
                                    android:src="@drawable/black_drop_down"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toStartOf="@+id/textView"
                                    app:layout_constraintHorizontal_bias="1.0"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                            </androidx.constraintlayout.widget.ConstraintLayout>

                            <LinearLayout
                                android:id="@+id/submittedRadio"
                                android:layout_width="14dp"
                                android:layout_height="14dp"
                                android:layout_marginTop="42dp"
                                android:background="@drawable/custom_radio"
                                android:orientation="vertical"
                                app:layout_constraintStart_toStartOf="@+id/guideline5"
                                app:layout_constraintTop_toBottomOf="@+id/sortIcon"></LinearLayout>

                            <TextView
                                android:id="@+id/submittedRadioTitle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="20dp"
                                android:layout_marginLeft="20dp"
                                android:layout_marginTop="40dp"
                                android:letterSpacing="0.06"
