<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Settings.SettingsLandingFragment">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/bottomNavigationView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/myProfileIcon"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginStart="40dp"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="60dp"
                android:src="@drawable/my_profile_icon_xhdpi"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/myProfileTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="18dp"
                android:layout_marginLeft="18dp"
                android:layout_marginTop="60dp"
                android:fontFamily="robo"
                android:letterSpacing="0.1"
                android:text="@string/my_profile"
                android:textColor="#595959"
                android:textSize="14dp"
                app:layout_constraintStart_toEndOf="@+id/myProfileIcon"
                app:layout_constraintTop_toTopOf="parent" />

            <View
                android:id="@+id/view"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="30dp"
                android:background="@drawable/horizontal_line"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/myProfileIcon" />

            <ImageView
                android:id="@+id/changePasswordIcon"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginStart="40dp"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="33dp"
                android:src="@drawable/change_password_icon_xhdpi"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view" />

            <TextView
                android:id="@+id/changePasswordTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="18dp"
                android:layout_marginLeft="18dp"
                android:layout_marginTop="33dp"
                android:fontFamily="robo"
                android:letterSpacing="0.06"
                android:text="@string/change_password"
                android:textColor="#595959"
                android:textSize="14dp"
                app:layout_constraintStart_toEndOf="@+id/changePasswordIcon"
                app:layout_constraintTop_toBottomOf="@+id/view" />

            <View
                android:id="@+id/view1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="33dp"
                android:background="@drawable/horizontal_line"
                app:layout_constraintTop_toBottomOf="@+id/changePasswordIcon"
                tools:layout_editor_absoluteX="-16dp" />


            <ImageView
                android:id="@+id/privacyPolicyIcon"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginStart="40dp"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="33dp"
                android:src="@drawable/privacy_policy_icon_xhdpi"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view1" />

            <TextView
                android:id="@+id/privacyPolicyTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="18dp"
                android:layout_marginLeft="18dp"
                android:layout_marginTop="33dp"
                android:fontFamily="robo"
                android:letterSpacing="0.06"
                android:text="@string/privacy_policy"
                android:textColor="#595959"
                android:textSize="14dp"
                app:layout_constraintStart_toEndOf="@+id/privacyPolicyIcon"
                app:layout_constraintTop_toBottomOf="@+id/view1" />

            <View
                android:id="@+id/view2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="33dp"
                android:background="@drawable/horizontal_line"
                app:layout_constraintTop_toBottomOf="@+id/privacyPolicyIcon"
                tools:layout_editor_absoluteX="0dp" />

            <ImageView
                android:id="@+id/termsAndConditionsIcon"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginStart="40dp"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="33dp"
                android:src="@drawable/terms_conditions_icon_xhdpi"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view2" />

            <TextView
                android:id="@+id/termsAndConditionsTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="18dp"
                android:layout_marginLeft="18dp"
                android:layout_marginTop="33dp"
                android:fontFamily="robo"
                android:letterSpacing="0.06"
                android:text="@string/term_conditions"
                android:textColor="#595959"
                android:textSize="14dp"
                app:layout_constraintStart_toEndOf="@+id/termsAndConditionsIcon"
                app:layout_constraintTop_toBottomOf="@+id/view2" />

            <View
                android:id="@+id/view3"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="33dp"
                android:background="@drawable/horizontal_line"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/termsAndConditionsIcon" />

            <ImageView
                android:id="@+id/notifivcationsIcon"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginStart="40dp"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="33dp"
                android:src="@drawable/notification_icon_xhdpi"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view3" />

            <TextView
                android:id="@+id/notifivcationsTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="18dp"
                android:layout_marginLeft="18dp"
                android:layout_marginTop="32dp"
                android:fontFamily="robo"
                android:letterSpacing="0.06"
                android:text="@string/notifications"
                android:textColor="#595959"
                android:textSize="14dp"
                app:layout_constraintStart_toEndOf="@+id/notifivcationsIcon"
                app:layout_constraintTop_toBottomOf="@+id/view3" />

            <View
                android:id="@+id/view4"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="33dp"
                android:background="@drawable/horizontal_line"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/notifivcationsIcon" />

            <ImageView
                android:id="@+id/logoutIcon"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginStart="40dp"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="33dp"
                android:src="@drawable/logout_icon_xhdpi"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view4" />

            <TextView
                android:id="@+id/logoutTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="18dp"
                android:layout_marginLeft="18dp"
                android:layout_marginTop="33dp"
                android:fontFamily="robo"
                android:letterSpacing="0.06"
                android:text="@string/log_out"
                android:textColor="#595959"
                android:textSize="14dp"
                app:layout_constraintStart_toEndOf="@+id/logoutIcon"
                app:layout_constraintTop_toBottomOf="@+id/view4" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/bottomNavigationView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/bottom_navigation"
            app:layout_constraintBottom_toBottomOf="@id/guideline2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <ImageView
                android:id="@+id/supportIcon"
                android:layout_width="21dp"
                android:layout_height="21dp"
                android:layout_marginStart="24dp"
                android:layout_marginLeft="24dp"
                android:layout_marginTop="8dp"
                android:src="@drawable/support_icon_xhdpi"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/supportTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="8dp"
                android:text="Support"
                android:textColor="#747474"
                android:textSize="8dp"
                app:fontFamily="@font/roboto"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/supportIcon" />

            <ImageView
                android:id="@+id/communityIcon"
                android:layout_width="21dp"
                android:layout_height="21dp"
                android:layout_marginStart="62dp"
                android:layout_marginLeft="62dp"
                android:layout_marginTop="8dp"
                android:src="@drawable/community_icon_xxhdpi"
                app:layout_constraintStart_toEndOf="@+id/supportIcon"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/communityTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="52dp"
                android:layout_marginLeft="52dp"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="8dp"
                android:text="Community"
                android:textColor="#747474"
                android:textSize="8dp"
                app:fontFamily="@font/roboto"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/supportIcon"
                app:layout_constraintTop_toBottomOf="@+id/communityIcon" />

            <ImageView
                android:id="@+id/accountsIcon"
                android:layout_width="21dp"
                android:layout_height="21dp"
                android:layout_marginStart="66dp"
                android:layout_marginLeft="66dp"
                android:layout_marginTop="8dp"
                android:src="@drawable/accounts_icon_xhdpi"
                app:layout_constraintStart_toEndOf="@+id/communityIcon"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/accountsTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="48dp"
                android:layout_marginLeft="48dp"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="8dp"
                android:text="Accounts"
                android:textColor="#747474"
                android:textSize="8dp"
                app:fontFamily="@font/roboto"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/communityTitle"
                app:layout_constraintTop_toBottomOf="@+id/accountsIcon" />

            <ImageView
                android:id="@+id/rewardsIcon"
                android:layout_width="21dp"
                android:layout_height="21dp"
                android:layout_marginStart="64dp"
                android:layout_marginLeft="64dp"
                android:layout_marginTop="8dp"
                android:src="@drawable/rewards_icon_xhdpi"
                app:layout_constraintStart_toEndOf="@+id/accountsIcon"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/rewardsTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="52dp"
                android:layout_marginLeft="52dp"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="8dp"
                android:text="Rewards"
                android:textColor="#747474"
                android:textSize="8dp"
                app:fontFamily="@font/roboto"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/accountsTitle"
                app:layout_constraintTop_toBottomOf="@+id/rewardsIcon" />

            <ImageView
                android:id="@+id/settingsIcon"
                android:layout_width="21dp"
                android:layout_height="21dp"
                android:layout_marginStart="60dp"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="8dp"
                android:src="@drawable/settings_icon_xxhdpi"
                app:layout_constraintStart_toEndOf="@+id/rewardsIcon"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/settingsTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="52dp"
                android:layout_marginLeft="52dp"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="8dp"
                android:text="Settings"
                android:textColor="@color/colorSkyBlue"
                android:textSize="8dp"
                app:fontFamily="@font/roboto"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/rewardsTitle"
                app:layout_constraintTop_toBottomOf="@+id/settingsIcon" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintGuide_begin="658dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>

