<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Support.SupportLandingFragment">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/subLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/colorWhite"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/bodyContent"
            android:layout_width="409dp"
            android:layout_height="677dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ScrollView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/help_title"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="60dp"
                            android:gravity="center"
                            android:text="@string/help_title"
                            android:textColor="#747474"
                            android:textSize="@dimen/hepl_title_size"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"></TextView>


                        <androidx.cardview.widget.CardView
                            android:id="@+id/findHelpCardView"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="32dp"
                            android:layout_marginLeft="32dp"
                            android:layout_marginTop="40dp"
                            android:layout_marginEnd="32dp"
                            android:layout_marginRight="32dp"
                            android:background="#FFFFFF"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/help_title">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:paddingBottom="30dp">

                                <TextView
                                    android:id="@+id/textView3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="34dp"
                                    android:layout_marginLeft="34dp"
                                    android:layout_marginTop="33dp"
                                    android:fontFamily="@font/roboto_medium"
                                    android:text="@string/find_help"
                                    android:textColor="#464646"
                                    android:textSize="@dimen/find_help_textsize"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent"></TextView>

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:id="@+id/searchLayout"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="32dp"
                                    android:layout_marginLeft="32dp"
                                    android:layout_marginTop="20dp"
                                    android:layout_marginEnd="32dp"
                                    android:layout_marginRight="32dp"
                                    android:background="@drawable/search"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintHorizontal_bias="0.0"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toBottomOf="@+id/textView3">

                                    <EditText
                                        android:id="@+id/search"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="20dp"
                                        android:layout_marginLeft="20dp"
                                        android:background="@color/colorWhite"
                                        android:ems="17"
                                        android:hint="@string/search"
                                        android:inputType="textPassword"
                                        android:letterSpacing="0.05"
                                        android:paddingTop="2dp"
                                        android:paddingBottom="2dp"
                                        android:textColorHint="#909090"
                                        android:textSize="10dp"
                                        app:layout_constraintBottom_toBottomOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"></EditText>

                                    <ImageView
                                        android:id="@+id/searchIcon"
                                        android:layout_width="18dp"
                                        android:layout_height="18dp"
                                        android:layout_marginTop="9dp"
                                        android:layout_marginEnd="12dp"
                                        android:layout_marginRight="12dp"
                                        android:layout_marginBottom="9dp"
                                        android:src="@drawable/search_icon_xxhdpi"
                                        app:layout_constraintBottom_toBottomOf="parent"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintTop_toTopOf="parent">

                                    </ImageView>

                                </androidx.constraintlayout.widget.ConstraintLayout>

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="6dp"
                                    android:layout_marginEnd="32dp"
                                    android:layout_marginRight="32dp"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintTop_toBottomOf="@+id/searchLayout">

                                    <TextView
                                        android:id="@+id/textView5"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginEnd="8dp"
                                        android:layout_marginRight="8dp"
                                        android:text="@string/Popular_Searches"
                                        android:textColor="#909090"
                                        android:textSize="10dp"
                                        app:layout_constraintEnd_toStartOf="@+id/imageView3"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <ImageView
                                        android:id="@+id/imageView3"
                                        android:layout_width="10dp"
                                        android:layout_height="10dp"
                                        android:layout_marginEnd="10dp"
                                        android:layout_marginRight="10dp"
                                        android:src="@drawable/drop_down_icon_xhdpi"
                                        app:layout_constraintBottom_toBottomOf="parent"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />
                                </androidx.constraintlayout.widget.ConstraintLayout>

                                <TextView
                                    android:id="@+id/exploreTitle"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="32dp"
                                    android:layout_marginLeft="32dp"
                                    android:layout_marginTop="54dp"
                                    android:fontFamily="@font/roboto_medium"
                                    android:text="@string/Explore"
                                    android:textColor="#464646"
                                    android:textSize="12dp"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toBottomOf="@+id/searchLayout" />

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toBottomOf="@+id/exploreTitle">

                                    <TextView
                                        android:id="@+id/textView9"
                                        android:layout_width="227dp"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="32dp"
                                        android:layout_marginLeft="32dp"
                                        android:layout_marginTop="8dp"
                                        android:letterSpacing="0.04"
                                        android:text="@string/explore_description"
                                        android:textColor="#909090"
                                        android:textSize="12dp"
                                        app:fontFamily="@font/roboto"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <ImageView
                                        android:id="@+id/imageView4"
                                        android:layout_width="28dp"
                                        android:layout_height="28dp"
                                        android:layout_marginStart="22dp"
                                        android:layout_marginLeft="22dp"
                                        android:layout_marginTop="14dp"
                                        android:src="@drawable/circle_icon_xhdpi"
                                        app:layout_constraintStart_toEndOf="@+id/textView9"
                                        app:layout_constraintTop_toTopOf="parent" />
                                </androidx.constraintlayout.widget.ConstraintLayout>


                            </androidx.constraintlayout.widget.ConstraintLayout>
                        </androidx.cardview.widget.CardView>


                        <androidx.cardview.widget.CardView
                            android:id="@+id/ticketsLayout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="32dp"
                            android:layout_marginLeft="32dp"
                            android:layout_marginTop="32dp"
                            android:layout_marginEnd="32dp"
                            android:layout_marginRight="32dp"
                            android:background="@drawable/find_help"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/findHelpCardView">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:paddingBottom="20dp">

                                <TextView
                                    android:id="@+id/tickestsTitle"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="34dp"
                                    android:layout_marginLeft="34dp"
                                    android:layout_marginTop="33dp"
                                    android:fontFamily="@font/roboto_medium"
                                    android:text="@string/tickets"
                                    android:textColor="#464646"
                                    android:textSize="@dimen/find_help_textsize"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent"></TextView>


                                <TextView
                                    android:id="@+id/tickestsDescription"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="32dp"
                                    android:layout_marginLeft="32dp"
                                    android:layout_marginTop="17dp"
                                    android:layout_marginEnd="32dp"
                                    android:layout_marginRight="32dp"
                                    android:letterSpacing="0.04"
                                    android:text="@string/tickets_description"
                                    android:textColor="#909090"
                                    android:textSize="12dp"
                                    app:fontFamily="@font/roboto"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toBottomOf="@+id/tickestsTitle" />

                                <View
                                    android:id="@+id/view2"
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:layout_marginTop="20dp"
                                    android:layout_marginEnd="80dp"
                                    android:layout_marginRight="80dp"
                                    android:background="#CCCCCC"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintHorizontal_bias="0.0"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toBottomOf="@+id/createTicketButton" />

                                <Button
                                    android:id="@+id/createTicketButton"
                                    android:layout_width="wrap_content"
                                    android:layout_height="30dp"
                                    android:layout_marginStart="32dp"
                                    android:layout_marginLeft="32dp"
                                    android:layout_marginTop="20dp"
                                    android:background="@drawable/change_password_button"
                                    android:fontFamily="@font/roboto_bold"
                                    android:paddingLeft="10dp"
                                    android:paddingRight="10dp"
                                    android:text="@string/create_tickets"
                                    android:textColor="@color/colorWhite"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toBottomOf="@+id/tickestsDescription" />

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:id="@+id/gotoTicketsLayout"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="16dp"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toBottomOf="@+id/view2">

                                    <TextView
                                        android:id="@+id/textView10"
                                        android:layout_width="6dp"
                                        android:layout_height="6dp"
                                        android:layout_marginTop="10dp"
                                        android:background="@drawable/red_small_circle"
                                        app:layout_constraintStart_toEndOf="@+id/textView6"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <TextView
                                        android:id="@+id/textView6"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="32dp"
                                        android:layout_marginLeft="32dp"
                                        android:layout_marginTop="12dp"
                                        android:fontFamily="@font/roboto_medium"
                                        android:text="@string/goto_your_tickets"
                                        android:textColor="#464646"
                                        android:textSize="12dp"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                </androidx.constraintlayout.widget.ConstraintLayout>

                                <ImageView
                                    android:id="@+id/circle_icon"
                                    android:layout_width="28dp"
                                    android:layout_height="28dp"
                                    android:layout_marginStart="32dp"
                                    android:layout_marginLeft="32dp"
                                    android:layout_marginTop="20dp"
                                    android:src="@drawable/circle_icon_xhdpi"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toBottomOf="@+id/gotoTicketsLayout" />


                            </androidx.constraintlayout.widget.ConstraintLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="32dp"
                            android:layout_marginLeft="32dp"
                            android:layout_marginTop="32dp"
                            android:layout_marginEnd="32dp"
                            android:layout_marginRight="32dp"
                            android:layout_marginBottom="82dp"
                            android:background="@drawable/find_help"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/ticketsLayout">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:id="@+id/phoneLayout"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:paddingBottom="30dp">

                                <TextView
                                    android:id="@+id/loremText"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="32dp"
                                    android:layout_marginLeft="32dp"
                                    android:layout_marginTop="24dp"
                                    android:layout_marginEnd="32dp"
                                    android:layout_marginRight="32dp"
                                    android:letterSpacing="0.04"
                                    android:text="@string/explore_description"
                                    android:textColor="#909090"
                                    android:textSize="12dp"
                                    app:fontFamily="@font/roboto"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:id="@+id/constraintLayout8"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="16dp"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toBottomOf="@+id/loremText">

                                    <ImageView
                                        android:id="@+id/imageView5"
                                        android:layout_width="16dp"
                                        android:layout_height="16dp"
                                        android:layout_marginStart="32dp"
                                        android:layout_marginLeft="32dp"
                                        android:layout_marginTop="10dp"
                                        android:layout_marginBottom="8dp"
                                        android:src="@drawable/call_icon_xhdpi"
                                        app:layout_constraintBottom_toBottomOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <TextView
                                        android:id="@+id/textView11"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="16dp"
                                        android:layout_marginLeft="16dp"
                                        android:layout_marginTop="8dp"
                                        android:layout_marginBottom="8dp"
                                        android:text="@string/phone_number"
                                        android:textColor="#303030"
                                        android:textSize="14dp"
                                        app:fontFamily="@font/roboto_black"
                                        app:layout_constraintBottom_toBottomOf="parent"
                                        app:layout_constraintStart_toEndOf="@+id/imageView5"
                                        app:layout_constraintTop_toTopOf="parent" />
                                </androidx.constraintlayout.widget.ConstraintLayout>

                                <TextView
                                    android:id="@+id/textView12"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="32dp"
                                    android:layout_marginLeft="32dp"
                                    android:layout_marginTop="8dp"
                                    android:text="@string/call_details"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toBottomOf="@+id/constraintLayout8" />

                            </androidx.constraintlayout.widget.ConstraintLayout>

                        </androidx.cardview.widget.CardView>

                    </androidx.constraintlayout.widget.ConstraintLayout>


                </LinearLayout>
            </ScrollView>

        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/bottomNavigationView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/bottom_navigation"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <ImageView
                android:id="@+id/supportIcon"
                android:layout_width="21dp"
                android:layout_height="21dp"
                android:layout_marginStart="24dp"
                android:layout_marginLeft="24dp"
                android:layout_marginTop="8dp"
                android:src="@drawable/support_icon_xhdpi"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/supportTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="8dp"
                android:text="Support"
                android:textColor="#747474"
                android:textSize="8dp"
                app:fontFamily="@font/roboto"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/supportIcon" />

            <ImageView
                android:id="@+id/communityIcon"
                android:layout_width="21dp"
                android:layout_height="21dp"
                android:layout_marginStart="62dp"
                android:layout_marginLeft="62dp"
                android:layout_marginTop="8dp"
                android:src="@drawable/community_icon_xxhdpi"
                app:layout_constraintStart_toEndOf="@+id/supportIcon"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/communityTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="52dp"
                android:layout_marginLeft="52dp"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="8dp"
                android:text="Community"
                android:textColor="#747474"
                android:textSize="8dp"
                app:fontFamily="@font/roboto"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/supportIcon"
                app:layout_constraintTop_toBottomOf="@+id/communityIcon" />

            <ImageView
                android:id="@+id/accountsIcon"
                android:layout_width="21dp"
                android:layout_height="21dp"
                android:layout_marginStart="66dp"
                android:layout_marginLeft="66dp"
                android:layout_marginTop="8dp"
                android:src="@drawable/accounts_icon_xhdpi"
                app:layout_constraintStart_toEndOf="@+id/communityIcon"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/accountsTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="48dp"
                android:layout_marginLeft="48dp"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="8dp"
                android:text="Accounts"
                android:textColor="#747474"
                android:textSize="8dp"
                app:fontFamily="@font/roboto"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/communityTitle"
                app:layout_constraintTop_toBottomOf="@+id/accountsIcon" />

            <ImageView
                android:id="@+id/rewardsIcon"
                android:layout_width="21dp"
                android:layout_height="21dp"
                android:layout_marginStart="64dp"
                android:layout_marginLeft="64dp"
                android:layout_marginTop="8dp"
                android:src="@drawable/rewards_icon_xhdpi"
                app:layout_constraintStart_toEndOf="@+id/accountsIcon"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/rewardsTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="52dp"
                android:layout_marginLeft="52dp"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="8dp"
                android:text="Rewards"
                android:textColor="#747474"
                android:textSize="8dp"
                app:fontFamily="@font/roboto"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/accountsTitle"
                app:layout_constraintTop_toBottomOf="@+id/rewardsIcon" />

            <ImageView
                android:id="@+id/settingsIcon"
                android:layout_width="21dp"
                android:layout_height="21dp"
                android:layout_marginStart="60dp"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="8dp"
                android:src="@drawable/settings_icon_xxhdpi"
                app:layout_constraintStart_toEndOf="@+id/rewardsIcon"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/settingsTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="50dp"
                android:layout_marginLeft="50dp"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="8dp"
                android:text="Settings"
                android:textColor="@color/colorSkyBlue"
                android:textSize="8dp"
                app:fontFamily="@font/roboto"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/rewardsTitle"
                app:layout_constraintTop_toBottomOf="@+id/settingsIcon" />
        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>